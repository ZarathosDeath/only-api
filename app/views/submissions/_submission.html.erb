<div id="<%= dom_id submission %>">
  <p>
    <strong>Name:</strong>
    <%= submission.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= submission.email %>
  </p>

  <p>
    <strong>Mobile phone:</strong>
    <%= submission.mobile_phone %>
  </p>

  <p>
    <strong>Resume:</strong>
    <%= submission.resume %>
  </p>

  <p>
    <strong>Blob,:</strong>
    <%= submission.blob, %>
  </p>

  <p>
    <strong>Job:</strong>
    <%= submission.job_id_id %>
